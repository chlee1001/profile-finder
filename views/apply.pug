extends layouts/main

block content
    .apply__container
        form(action=routes.apply, method="post")
            each item in resumes
                +resumeBlock({
                    id:item.id,
                    title:item.title,
                    image:item.imgUrl1,
                    creator:item.creator,
                })
                input(type="radio" name="myResumes" value=item.id)
            input(type="submit", value="지원하기")